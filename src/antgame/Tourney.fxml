<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="440.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="antgame.Controller">
    <stylesheets>
        <URL value="@fxml.css" />
    </stylesheets>
   <children>
      <TextField fx:id="numOfPlayersTA" layoutX="225.0" layoutY="235.0" prefHeight="31.0" prefWidth="201.0" />
      <Label layoutX="239.0" layoutY="151.0" prefHeight="73.0" prefWidth="174.0" text="Number of Players" textAlignment="CENTER" textFill="#329a43" wrapText="true">
         <font>
            <Font name="Vivaldi Italic" size="24.0" />
         </font>
      </Label>
      <Button fx:id="homeButton" layoutX="195.0" layoutY="14.0" mnemonicParsing="false" onAction="#goHome" text="Home" />
      <Button fx:id="worldButton" layoutX="170.0" layoutY="455.0" mnemonicParsing="false" onAction="#importWorld" text="Import World" />
      <Button fx:id="showButton" layoutX="14.0" layoutY="455.0" mnemonicParsing="false" onAction="#showPairings" text="Show Pairings" />
      <Button fx:id="playButton" layoutX="326.0" layoutY="455.0" mnemonicParsing="false" onAction="#playGame" text="Start Game" />
      <Button fx:id="loadButton" layoutX="281.0" layoutY="281.0" mnemonicParsing="false" onAction="#loadPlayers" text="Load Players" />
      <Label fx:id="launcherLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="225.0" layoutY="49.0" prefHeight="102.0" prefWidth="201.0" textAlignment="CENTER">
         <font>
            <Font size="9.0" />
         </font></Label>
      <TableView fx:id="pairingTable" layoutX="4.0" layoutY="61.0" prefHeight="374.0" prefWidth="189.0">
        <columns>
          <TableColumn fx:id="redColumn" editable="false" prefWidth="63.0" resizable="false" sortable="false" text="Red" />
          <TableColumn fx:id="blackColumn" prefWidth="63.0" resizable="false" text="Black" />
            <TableColumn fx:id="worldColumn" prefWidth="63.0" resizable="false" text="World" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
